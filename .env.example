# Embedding LLM Service (OpenAI-compatible)
EMBEDDING_LLM_BASE_URL=<your-embedding-llm-base-url>
EMBEDDING_LLM_API_KEY=<your-embedding-llm-api-key>

# Reranker LLM Service (OpenAI-compatible)
RERANKER_LLM_BASE_URL=<your-reranker-llm-base-url>
RERANKER_LLM_API_KEY=<your-reranker-llm-api-key>

# Experience LLM Service
EXPERIENCE_LLM_BASE_URL=<your-experience-llm-base-url>
EXPERIENCE_LLM_API_KEY=<your-experience-llm-api-key>

# Model Configuration (optional, has defaults)
EMBEDDING_MODEL=Qwen3-Embedding-8B
RERANKER_MODEL=Qwen3-Reranker-8B
EXPERIENCE_LLM_MODEL=gpt-4o

# ChromaDB Path (optional)
CHROMA_DB_PATH=./memory_system/data/chroma

# Retrieval Configuration (optional)
DEFAULT_TOP_K=20
DEFAULT_TOP_N=3
EMBEDDING_DIMENSION=1024

# Logging (optional)
FAILED_LEARNING_LOG_PATH=./memory_system/logs/failed_learning_tasks.jsonl
